{
  "postBuild": {
    "commands": [
      "sudo apt update",
      "sudo apt install -y libmariadb-dev"
    ]
  },
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }